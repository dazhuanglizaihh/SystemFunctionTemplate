<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.UserMapper">

    <!-- 添加一个用户信息 -->
    <insert id="addUser" parameterType="Po.UserMapper">
        insert into user
        (id,name,email,password,createtime)
        values (#{id},#{name},#{email},#{password},#{cretetime})
    </insert>

</mapper>